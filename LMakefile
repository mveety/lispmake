(:package lispmake)
(:toplevel main)
(:quicklisp unix-options split-sequence osicat cl-fad)
(:lisp all)
(:build-with "/usr/local/bin/sbcl")
(:do-build)
(:file "package.lisp"
	"vars.lisp"
	"debug.lisp"
	"lisp-specific.lisp"
	"plugin.lisp"
	"options.lisp"
	"core.lisp"
	"file-utils.lisp"
	"lispmake.lisp")
(:output "lispmake")
(:generate)
